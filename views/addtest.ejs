<!doctype html>
<html lang="tr">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-9ndCyUaIbzAi2FUVXJi0CjmCapSmO7SnpJef0486qhLnuZ2cdeRhO02iuK6FUUVM" crossorigin="anonymous">
    <link rel="stylesheet" href="assets/css/font-awesome.min.css">
  </head>
  <body>

    <div class="container">
        <form class="row g-3" method="post" action="/admin/test/add">
            <div class="col-md-12">
              <label for="inputEmail4" class="form-label">TEST TÄ°TLE</label>
              <input type="text" class="form-control" name="title" id="inputEmail4">
            </div>
            <div class="col-md-12">
                <label for="inputEmail4" class="form-label">TEST CATEGORY</label>
                <input type="text" class="form-control" name="category" id="inputEmail4">
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">TEST LANG</label>
                <input type="text" class="form-control" name="lang" id="inputEmail4">
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">REQUIRED IQ</label>
                <input type="text" class="form-control" name="requirediq" id="inputEmail4">
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">REQUIRED TEST ID</label>
                <select class="form-control" name="requiredtestid" id="">
                    <option value="0">No Required Test</option>
                    <% Object.keys(data).forEach(function(prop) { %>
                        <option value="<%- data[prop]['testid']%>"><%- data[prop]["title"] %></option>                       
                     <% }); %>
                </select>
                
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">REQUIRED XP</label>
                <input type="text" class="form-control" name="requiredxp" id="inputEmail4">
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">TIMELIMIT</label>
                <input type="text" class="form-control" name="timelimit" id="inputEmail4">
              </div>

              <div class="col-md-12">
                <label for="inputEmail4" class="form-label">XP GIVEN</label>
                <input type="text" class="form-control" name="xp" id="inputEmail4">
              </div>
              <p>QUEUE 1 VERSION 1 CREATEDDATE UPDATEDDATE  QUESTIONS(ARRAY)
              </p>
                
            <div class="col-12">
              <button type="submit" class="btn btn-primary">EKLE</button>
            </div>
          </form>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js" integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz" crossorigin="anonymous"></script>
  </body>
</html>